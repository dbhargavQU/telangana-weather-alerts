{
  "name": "telangana-weather-live",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"commonjs\"}' prisma/seed.ts",
    "worker": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"commonjs\"}' scripts/worker.ts",
    "jobs:run-once": "ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"commonjs\"}' scripts/run-jobs-once.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "^5.51.1",
    "@vercel/og": "^0.6.3",
    "bullmq": "^5.8.0",
    "cheerio": "^1.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "fastify": "^4.28.1",
    "ioredis": "^5.4.1",
    "leaflet": "^1.9.4",
    "react-leaflet": "^4.2.1",
    "lucide-react": "^0.454.0",
    "next": "15.0.0-rc.1",
    "openai": "^4.57.0",
    "pngjs": "^7.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.4",
    "shiki": "^1.15.2",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/pngjs": "^6.0.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-config-next": "15.0.0-rc.1",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prisma": "^5.17.0",
    "tailwindcss": "^3.4.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "@types/leaflet": "^1.9.12",
    "vitest": "^2.0.5"
  }
}


